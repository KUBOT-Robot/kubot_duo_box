<launch>
    <node name="kubot_due_box" pkg="kubot_due_box" type="kubot_due_box" output="screen">
        <param name="port" value="/dev/ttyACM0" />
        <param name="baudrate" value="115200" />
        <param name="led_status_topic" value="led_status" />
        <param name="led_control_topic" value="led_control" />
        <param name="servo_status_topic" value="servo_status" />
        <param name="servo_control_topic" value="servo_status" />
        <param name="led_control_enable" value="true" />
        <param name="servo_control_enable" value="true"/>

        <param name="publish_tf" value="$(arg publish_tf)" />
    </node>
</launch>